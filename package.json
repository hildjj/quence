{
  "name": "quence",
  "description": "Create sequence diagrams",
  "version": "4.0.2",
  "type": "module",
  "main": "./lib/index.js",
  "bin": {
    "quence": "bin/quence.js"
  },
  "files": [
    "bin/*",
    "lib/*",
    "types/*"
  ],
  "types": "types/lib/index.d.ts",
  "homepage": "https://github.com/hildjj/quence",
  "bugs": "https://github.com/hildjj/quence/issues",
  "scripts": {
    "clean": "rm -rf coverage types && cd web && npm run clean",
    "docs": "cd web && npm run build",
    "lint": "eslint .",
    "test": "c8 ava",
    "snaps": "ava --update-snapshots",
    "build": "peggy --format es lib/grammar.pegjs && npm run types && npm run docs",
    "start": "hostlocal",
    "types": "tsc",
    "version": "package-extract -s name homepage version && npm run snaps && npm run build && (cd examples && make clean && make) && (cd web && npm run clean && npm run build) && git add examples test/snapshots package.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/hildjj/quence.git"
  },
  "keywords": [
    "sequencediagram",
    "sequence",
    "diagram"
  ],
  "license": "BSD-2-Clause",
  "readmeFilename": "README.md",
  "contributors": [
    {
      "name": "Cullen Jennings",
      "email": "fluffy@iii.ca"
    },
    {
      "name": "Eric Rescorla",
      "email": "ekr@rtfm.com"
    },
    {
      "name": "Martin Thomson",
      "email": "martin.thomson@gmail.com"
    },
    {
      "name": "Joe Hildebrand",
      "email": "joe-github@cursive.net"
    }
  ],
  "dependencies": {
    "commander": "14.0.0",
    "pdfkit": "^0.17.1",
    "xmlbuilder2": "3.1.1"
  },
  "devDependencies": {
    "@cto.af/eslint-config": "^6.0.13",
    "@peggyjs/coverage": "2.0.4",
    "@types/node": "24.2.0",
    "@types/pdfkit": "0.17.2",
    "ava": "^6.4.1",
    "c8": "^10.1.3",
    "eslint": "9.32.0",
    "eslint-plugin-ava": "^15.1.0",
    "execa": "^9.6.0",
    "hostlocal": "2.1.4",
    "package-extract": "^3.1.3",
    "peggy": "^5.0.6",
    "typescript": "^5.9.2",
    "typescript-eslint": "8.39.0"
  },
  "packageManager": "pnpm@10.14.0",
  "engines": {
    "node": ">=20"
  }
}
